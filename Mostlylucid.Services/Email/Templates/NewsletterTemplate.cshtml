@using Mostlylucid.Shared.Helpers
@using RazorLight
@inherits TemplatePage< Mostlylucid.Shared.Models.EmailSubscription.EmailRenderingModel>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>mostlylucid @Model.SubscriptionType.EnumDisplayName() Blog Update</title>
    <style>
        body {
            background-color: #1D232A;
            color: white;
            font-family: Arial, sans-serif;
            margin: 0;
            padding: 0;
        }
        .container {
            max-width: 800px;
            margin: 0 auto;
            padding: 20px;
        }
        .header {
            text-align: center;
            padding: 20px 0;
        }
        .header img {
            width: 200px;
        }
        .header h1 {
            font-size: 24px;
            color: #fff;
        }
        .content {
            background-color: #2B323B;
            padding: 20px;
            border-radius: 8px;
        }
        .blog-post {
            padding: 15px;
            border-bottom: 1px solid #3C434D;
        }
        .blog-post:last-child {
            border-bottom: none;
        }
        .blog-title {
            font-size: 18px;
            color: #66C2FF;
            margin: 0;
        }
        .blog-date {
            font-size: 14px;
            color: #aaa;
            margin: 5px 0;
        }
        .blog-description {
            font-size: 16px;
            color: #ccc;
        }
        .footer {
            text-align: center;
            padding: 20px;
            font-size: 14px;
            color: #888;
        }
        a {
            color: #66C2FF;
            text-decoration: none;
        }
    </style>
</head>
<body>
<div class="container">
    <div class="header">
        <img src="data:image/svg+xml;base64," alt="Logo">
        <h1>mostlylucid @Model.SubscriptionType.EnumDisplayName() Blog Update</h1>
    </div>
    <div class="content">
        @foreach (var post in Model.Posts)
        {
            <div class="blog-post">
                <h2 class="blog-title">@post.Title</h2>
                <p class="blog-date">Published on: @post.PublishedDate.ToString("dddd, d\\t\\h MMMM 'at' h:mmtt")</p>
                <p class="blog-description">@post.PlainTextContent.TruncateAtWord(200)</p>
            </div>
     
        }
        <!-- Add more blog posts as needed -->
    </div>
    <div class="footer">
        <p>You're receiving this email because you subscribed to our blog updates.</p>
        <p><a href="@Model.UnsubscribeUrl">Unsubscribe</a></p> <p><a href="@Model.ManageSubscriptionUrl">Manage Subscription</a></p>
    </div>
</div>
</body>
</html>