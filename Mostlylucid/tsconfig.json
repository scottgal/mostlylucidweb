{
  "compilerOptions": {
    "target": "es2017",
    "module": "esNext",
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "skipLibCheck": true,
    "preserveConstEnums":true,
    "sourceMap": true,
    "noResolve": false,
    "removeComments": true,
    "experimentalDecorators": true,
    "lib": [
      "dom",
      "es2015",
      "es2015.promise",
      "es5"
    ],
    "declaration": true
  }
}